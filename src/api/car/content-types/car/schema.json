{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "registrationPlate": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "GREEN",
        "GRAY",
        "BLACK",
        "WHITE",
        "RED",
        "BLUE",
        "BROWN",
        "YELLOW",
        "ORANGE",
        "PURPLE",
        "PINK",
        "BEIGE",
        "CYAN",
        "MAGENTA",
        "LIME",
        "TEAL",
        "OLIVE",
        "MAROON",
        "NAVY",
        "SILVER"
      ],
      "required": false
    },
    "chassis": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "seats": {
      "type": "integer",
      "required": false,
      "min": 0
    },
    "doors": {
      "type": "integer",
      "required": false,
      "min": 0
    },
    "smallBaggage": {
      "type": "integer",
      "min": 0,
      "required": false
    },
    "largeBaggage": {
      "type": "integer",
      "min": 0
    },
    "isAvailable": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "registeredUntil": {
      "type": "datetime",
      "required": false
    },
    "year": {
      "type": "integer",
      "min": 1900,
      "required": false
    },
    "cubicSize": {
      "type": "integer",
      "min": 0,
      "required": false
    },
    "make": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "carType": {
      "type": "enumeration",
      "enum": [
        "MINI_VAN",
        "SUV",
        "LUXURY",
        "SMALL",
        "COMPACT",
        "MEDIUM",
        "LARGE",
        "ESTATE",
        "VAN"
      ]
    },
    "fuelType": {
      "type": "enumeration",
      "enum": [
        "DIESEL",
        "PETROL",
        "HYBRID",
        "ELECTRIC"
      ]
    },
    "transmissionType": {
      "type": "enumeration",
      "enum": [
        "AUTOMATIC",
        "MANUAL"
      ]
    },
    "carGroup": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::car-group.car-group",
      "inversedBy": "cars"
    },
    "userGroup": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::multi-tenant.user-group",
      "inversedBy": "cars"
    },
    "horsePower": {
      "type": "integer",
      "min": 0
    },
    "mileages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mileage.mileage"
    },
    "isSold": {
      "type": "boolean",
      "default": false
    }
  }
}
